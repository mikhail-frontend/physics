{
  "name": "uk-seo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern=server",
    "start": "nuxt start --modern=server",
    "generate": "nuxt generate",
    "eslint": "yarn eslintVue && yarn eslintTS",
    "eslintTS": "npx eslint --fix . --ext .ts",
    "eslintVue": "npx eslint --fix . --ext .vue",
    "test:unit": "jest",
    "prod": "rimraf dist && yarn && yarn build && yarn start",
    "lint:scss": "stylelint **/*.scss",
    "lint:fix-scss": "stylelint --fix **/*.scss",
    "deploy": "yarn build && yarn generate && firebase deploy"
  },
  "dependencies": {
    "@nuxt/image": "^0.6.2",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/proxy": "^2.1.0",
    "@silvermine/videojs-quality-selector": "^1.2.5",
    "@types/video.js": "^7.3.33",
    "@vkontakte/superappkit": "^1.45.0",
    "autoprefixer": "^10.4.14",
    "core-js": "^3.15.1",
    "css-byebye": "^3.1.0",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.0",
    "html2canvas": "^1.4.1",
    "javascript-state-machine": "^3.1.0",
    "nanoid": "^3.1.23",
    "nuxt": "^2.15.7",
    "nuxt-dropzone": "^1.0.4",
    "nuxt-property-decorator": "^2.9.1",
    "nuxt-speedkit": "^2.0.12",
    "popper.js": "^1.16.1",
    "sass": "^1.49.11",
    "sass-loader": "10",
    "universal-social-auth": "^0.2.6",
    "v-copy": "^0.1.0",
    "v-mask": "^2.3.0",
    "video.js": "^7.18.1",
    "vue-masonry-css": "^1.0.3",
    "vue-ssr-carousel": "^1.4.0",
    "vue-trend-chart": "^0.15.3",
    "vue2-dropzone": "^3.6.0",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@mdi/font": "^6.6.96",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config": "^9.0.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^8.13.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nuxt-purgecss": "^1.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-twbs-bootstrap": "^7.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "stylelint-declaration-strict-value": "^1.9.1",
    "stylelint-no-px": "^1.0.1",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-jest": "^29.0.5",
    "vue-eslint-parser": "^8.3.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.7.14",
    "vuetify-loader": "^1.7.3",
    "vuex-mock-store": "^0.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "vue",
      "js",
      "ts",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@nuxt)/",
      "/node_modules/(?!vuetify/lib)",
      "node_modules/(?!@vuetify|(?!deck.gl)|ng-dynamic)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}

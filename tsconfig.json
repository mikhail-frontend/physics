{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": false,
    "noEmit": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "*": [
        "./node_modules/*"
      ],
      "@img": [
        "./assets/img"
      ]
    },
    "types": [
      "@nuxt/types",
      "jest",
      "@nuxtjs/axios",
      "@nuxtjs/vuetify",
      "@nuxt/image",
      "@types/node"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "src"
  ]
}
